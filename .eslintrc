{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
  },
  "extends": ["airbnb/base"],
  "rules": {
    "semi": "off",
    "no-plusplus": "off",
    "eol-last": "off",
    "max-len": ["error", 150, { "igonreUrls": true }],
    "no-tabs": "off",
    "no-alert": "off",
    "prefer-destructuring": "off",
    "indient": [2, "tab"],
    "no-param-reassign": "off",
  },
}
